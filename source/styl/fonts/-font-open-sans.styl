@font-face
    font-weight 400
    font-style normal
    font-family 'Open Sans'
    src local('Open Sans'), local('OpenSans-Regular'),
    url('./font-open-sans_normal.woff') format('woff')

@font-face
    font-weight 400
    font-style italic
    font-family 'Open Sans'
    src local('Open Sans Italic'), local('OpenSans-Italic'),
    url('./font-open-sans_italic.woff') format('woff')

@font-face
    font-weight 700
    font-style normal
    font-family 'Open Sans'
    src local('Open Sans Bold'), local('OpenSans-Bold'),
    url('./font-open-sans_bold.woff') format('woff')
